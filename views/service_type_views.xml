<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <record id="service_type_tree_view" model="ir.ui.view">
        <field name="name">dsl.service.type.tree</field>
        <field name="model">dsl.service.type</field>
        <field name="arch" type="xml">
            <tree string="" editable="bottom">
                <field name="name"></field>
                <field name="note"></field>
            </tree>
        </field>
    </record>

    <record id="service_type_action_view" model="ir.actions.act_window">
        <field name="name">Service Type</field>
        <field name="res_model">dsl.service.type</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>